<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imalok.superpowerlearning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".FitnessActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MoneyActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WeightlossActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Bounceback"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BackPainReliefActivity"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <service android:name=".FitnessMusicService" />
        <service android:name=".MoneyMusicService" />
    </application>

</manifest>